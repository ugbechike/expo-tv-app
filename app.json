{
  "expo": {
    "scheme": "routertv",
    "plugins": [
      [
        "@react-native-tvos/config-tv",
        {
          "androidTVBanner": "./assets/tv_icons/icon-400x240.png",
          "appleTVImages": {
            "icon": "./assets/tv_icons/icon-1280x768.png",
            "iconSmall": "./assets/tv_icons/icon-400x240.png",
            "iconSmall2x": "./assets/tv_icons/icon-800x480.png",
            "topShelf": "./assets/tv_icons/icon-1920x720.png",
            "topShelf2x": "./assets/tv_icons/icon-3840x1440.png",
            "topShelfWide": "./assets/tv_icons/icon-2320x720.png",
            "topShelfWide2x": "./assets/tv_icons/icon-4640x1440.png"
          }
        }
      ],
      [
        "expo-router",
        {
          "origin": "https://routertv.expo.app"
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.router",
          "publishableKey": ""
        }
      ],
      "expo-font",
      "expo-video"
    ],
    "android": {
      "splash": {
        "image": "./assets/images/splash.png"
      },
      "package": "com.chycocee1.MyTVProject"
    },
    "ios": {
      "splash": {
        "image": "./assets/images/splash.png"
      },
      "bundleIdentifier": "com.chycocee1.MyTVProject",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png",
      "output": "server"
    },
    "experiments": {
      "typedRoutes": true
    },
    "newArchEnabled": true,
    "name": "MyTVProject",
    "slug": "MyTVProject",
    "extra": {
      "router": {
        "origin": "https://routertv.expo.app"
      },
      "eas": {
        "projectId": "b54bf413-311f-4162-939b-dec0cf9fab61"
      }
    }
  }
}
